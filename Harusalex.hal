  end else begin
    EditField(h1,v+=vs,50,"  1",Normal,BaseRate1,true,0);
    EditField(h1,v+=vs,50,"  2",Normal,BaseRate2,true,0);
  end;
  end;
  Tile(tilenr,30,false,tilenr,Comment); tilenr = tilenr + 1;
